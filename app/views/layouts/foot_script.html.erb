<script type="text/javascript" charset="utf-8">
  // <%= params.to_json.html_safe %>
  var js_class = '<%= params[:controller] %>';
  var init_method = '<%= params[:action] %>'
  js_class = js_class.replace('\/','_');
  try{
    this.cjp = eval(js_class);
  } catch (e){
  };



  if( this.cjp && typeof this.cjp.init == "function" ) {
    this.cjp.init();
  }
  if( this.cjp ){
    try{
      eval('this.cjp.'+ init_method +'()');
    } catch (e) {

    }
  }
</script>

